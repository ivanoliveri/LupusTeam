  <!-- Fav and touch icons -->
  <link rel="apple-touch-icon-precomposed" sizes="144x144" href="../../Content/img/apple-touch-icon-144-precomposed.png">
  <link rel="apple-touch-icon-precomposed" sizes="114x114" href="../../Content/img/apple-touch-icon-114-precomposed.png">
  <link rel="apple-touch-icon-precomposed" sizes="72x72" href="../../Content/img/apple-touch-icon-72-precomposed.png">
  <link rel="apple-touch-icon-precomposed" href="../../Content/img/apple-touch-icon-57-precomposed.png">
  <link rel="shortcut icon" href="../../Content/img/favicon.ico">
  <style type="text/css">
    #wrap {
      min-height: 100%;
    }

    #main {
      overflow:auto;
      padding-bottom:130px;
    }
    #main li {
        margin-top: 15px;
    }
    .footer {
      position: relative;
      margin-top: -140px; 
      height: 120px;
      clear:both;
      padding-top:20px;
    } 
  </style>
  
  <script type="text/javascript" src="../../Content/js/jquery-2.0.3.js"></script>
  <script type="text/javascript" src="../../Content/js/jquery.min.js"></script>
  <script type="text/javascript" src="../../Content/js/bootstrap.min.js"></script>
  <script type="text/javascript" src="../../Content/js/scripts.js"></script>
  <script type="text/javascript">
      $(document).ready(function () {
          var title = $(this).attr('title').toString();
          switch (title) {
              case "Inicio | Lupus' Team":
                  $('#Inicio').addClass('active');
                  break;
              case "Contacto | Lupus' Team":
                  $('#Contacto').addClass('active');
                  break;
              case "Quienes Somos? | Lupus' Team":
                  $('#QuienesSomos').addClass('active');
                  break;
              default:
                  $('#Tecnologias').addClass('active');
                  break;
          }
      });
      $(document).ready(function () {
          
      });
  </script>
  
</head>

<body>
    <div class="container-fluid">
        <div class="row-fluid">
            <div class="span12">
                <div class="navbar">
                    <div class="navbar-inner">
                        <div class="container-fluid">
                            <img alt="100x100" style="float:left" src="../../Content/img/lupus40x40.png"/>
                            <a data-target=".navbar-responsive-collapse" data-toggle="collapse" class="btn btn-navbar"><span class="icon-bar"></span><span class="icon-bar"></span><span class="icon-bar"></span></a><a href="#" class="brand">Lupus' Team</a>
                            <div class="nav-collapse collapse navbar-responsive-collapse">
                                <ul class="nav">
                                    <li id="Inicio">
                                        <a href="@Url.Action("Index", "Home")">Inicio</a>
                                    </li>
                                    <li id="Contacto">
                                        <a href="@Url.Action("Index", "Contact")">Contacto</a>
                                    </li>
                                    <li id="QuienesSomos">
                                        <a href="@Url.Action("Index", "AboutUs")">Quienes Somos?</a>
                                    </li>
                                    <li id="Tecnologias" class="dropdown">
                                        <a data-toggle="dropdown" class="dropdown-toggle" href="#">Teconologías<strong class="caret"></strong></a>
                                        <ul class="dropdown-menu">
                                            <li>
                                                <a href="@Url.Action("IndexBackend", "Tecnology")">Backend</a>
                                            </li>
                                            <li>
                                                <a href="@Url.Action("IndexFrontend", "Tecnology")">Frontend</a>
                                            </li>
                                            <li>
                                                <a href="@Url.Action("IndexDataBase", "Tecnology")">Bases de Datos</a>
                                            </li>
                                        </ul>
                                    </li>
                                </ul>
                        
                            </div>
						
                        </div>
                    </div>
				
                </div>
            </div>
        </div>
    </div>
    <div>
        @RenderBody()
    </div>
    <div id="wrap">
        <div id="main" class="container-fluid" style="background: #303337;color:white;position:relative;display: block;bottom:0;left:0;">
            <div class="row-fluid">
                <div class="span2">
                    <ul class="unstyled" style="line-spacing: 2.5em;font-size: 15px;">
                        <br/>
                        <li>
                            <a style="color:whitesmoke;text-decoration: none;" href="@Url.Action("Index", "Home")">INICIO</a>
                        </li>
                        <li>
                            <a style="color:whitesmoke;text-decoration: none;" href="@Url.Action("Index", "Contact")">CONTACTO</a>
                        </li>
                        <li>
                            <a style="color:whitesmoke;text-decoration: none;" href="@Url.Action("Index", "AboutUs")">QUIENES SOMOS?</a>
                        </li>
                        <li>
                            <a style="color:whitesmoke;text-decoration: none;" href="@Url.Action("IndexBackend", "Tecnology")">BACKEND</a>
                        </li>
                        <li>
                            <a style="color:whitesmoke;text-decoration: none;" href="@Url.Action("IndexFrontend", "Tecnology")">FRONTEND</a>
                        </li>
                        <li>
                            <a style="color:whitesmoke;text-decoration: none;" href="@Url.Action("IndexDataBase", "Tecnology")">BASE DE DATOS</a>
                        </li>
                    </ul>
                </div>
                <div class="span7">
                    <h2>
                        Tenes alguna idea en mente?
                    </h2>
                    <p>
                        No dudes en ponerte en contacto con nosotros!
                    </p>
                    <p>
                        <a class="btn" href="@Url.Action("Index", "Contact")">Contactar</a>
                    </p>
                </div>
                <div class="span3" style="float:right;">
                    <h3>
                        Lupus' Team
                    </h3>
                    <img src="../../Content/img/apple-touch-icon-144-precomposed.png"/>
                </div>
            </div>
        </div>
    </div>
    <footer class="footer"></footer>
</body>
</html>